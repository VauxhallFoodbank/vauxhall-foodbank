{
  "name": "norwood-and-brixton-foodbank",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir . --ignore-path .gitignore && npm run check_types",
    "lint_fix": "next lint --fix --dir . --ignore-path .gitignore && npm run check_types",
    "check_types": "tsc && echo 'No typescript errors!'",
    "open:cypress": "start-server-and-test 'next start -p 3100' http://localhost:3100 'npx cypress open --config baseUrl=http://localhost:3100'",
    "test": "start-server-and-test test:runner_server_release http://localhost:3200 test:runner",
    "test:component": "start-server-and-test test:runner_server_release http://localhost:3200 test:runner_component",
    "test:e2e": "start-server-and-test test:runner_server_release http://localhost:3200 test:runner_e2e",
    "test:coverage": "npm run coverage:generate && npm run coverage:run_tests && npm run coverage:report",
    "test:runner": "npm run test:runner_component && npm run test:runner_e2e",
    "test:runner_component": "npx cypress run --component",
    "test:runner_e2e": "npx cypress run",
    "test:runner_server_dev": "next dev -p 3200",
    "test:runner_server_release": "next start -p 3200",
    "coverage:generate": "nyc instrument --delete src .nyc_output --compact=false --all",
    "coverage:run_tests": "start-server-and-test coverage:server http://localhost:3200 test:runner",
    "coverage:server": "nyc npm run test:runner_server_dev",
    "coverage:report": "nyc report --reporter html && nyc report --reporter text-summary",
    "dev:create_admin_and_caller_users": "tsc supabase/tools/createAdminAndCallerUsers.ts && node supabase/tools/createAdminAndCallerUsers.js",
    "dev:create_bucket": "tsc supabase/tools/createStorage.ts && node supabase/tools/createStorage.js",
    "dev:reset_supabase": "supabase db reset && npm run dev:create_admin_and_caller_users && npm run dev:create_bucket",
    "db:local:generate_types": "supabase gen types typescript --local --schema public > src/databaseTypesFile.ts",
    "db:remote:generate_types": "supabase gen types typescript --schema public > src/databaseTypesFile.ts",
    "db:generate_seed": "npx tsx supabase/seed/seed.mts > supabase/seed.sql"
  },
  "nyc": {
    "all": true,
    "extends": "@istanbuljs/nyc-config-typescript"
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.11.0",
    "@emotion/is-prop-valid": "^1.2.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/react": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.8",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@mui/x-data-grid": "^6.19.6",
    "@mui/x-date-pickers": "^6.10.2",
    "@react-pdf/renderer": "^3.1.12",
    "@supabase/auth-helpers-nextjs": "^0.7.2",
    "@supabase/gotrue-js": "^2.31.0",
    "@supabase/supabase-js": "^2.26.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/cypress-dotenv": "^2.0.1",
    "@types/node": "20.3.1",
    "@types/pdf-parse": "^1.1.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-modal": "^3.16.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "axe-core": "^4.7.2",
    "cypress": "^13.1.0",
    "cypress-axe": "^1.5.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "encoding": "^0.1.13",
    "eslint": "^8.43.0",
    "eslint-config-next": "^13.4.6",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "next": "^13.4.19",
    "nyc": "^15.1.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-data-table-component": "^7.5.3",
    "react-dom": "18.2.0",
    "react-modal": "^3.16.1",
    "start-server-and-test": "^2.0.0",
    "styled-components": "^6.0.4",
    "supabase": "^1.142.2",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "winston-cloudwatch": "^6.2.0"
  },
  "devDependencies": {
    "@snaplet/copycat": "^5.0.0",
    "@snaplet/seed": "^0.88.2",
    "@types/seedrandom": "^3.0.8",
    "@types/uuid": "^9.0.8",
    "seedrandom": "^3.0.5",
    "snaplet": "^0.88.2"
  }
}
